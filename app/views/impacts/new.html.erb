<div class="hero container">
	<div class="row">
		<div class="col-10">
			<h1>Add an Impact</h1>
			<%= simple_form_for @impact do |f| %>
		    <%= f.input :name %>
		    <%= f.input :quantity %>
		    <%= f.association :items %>
		    <%= f.submit 'Create', class: 'btn btn-primary' %>
			<% end %>
		</div>
	</div>
</div>